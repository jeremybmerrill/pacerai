[Unit]
Description=Run rss_search_warrant_alerter hourly
Requires=rss_search_warrant_alerter.service
[Timer]
Unit=rss_search_warrant_alerter.service
OnUnitInactiveSec=1d
OnCalendar=Mon..Fri *-*-* 1,2,3,14..23:00:00 # UTC. Runs until 3am UTC because that's 11pm ET which is 5pm Hawaii.
RandomizedDelaySec=1m
AccuracySec=60s

[Install]
WantedBy=timers.target
